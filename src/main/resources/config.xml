<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    ">
    <bean id="responseHandler" class="webpagedownloader.client.WebpageResponseHandler" autowire="byName"/>
    <bean id="webpageFetcher" class="webpagedownloader.client.WebpageFetcher" autowire="byName"/>

    <bean id="fileManager" class="webpagedownloader.FileManager" autowire="byName"/>
</beans>
